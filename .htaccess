Options +FollowSymLinks
RewriteEngine on

DirectoryIndex index.html

php_value display_errors 1
php_flag output_buffering on

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} !^www\. [NC]
RewriteCond %{HTTP_HOST} ^(?:www\.)?(.+)$ [NC]
RewriteRule ^ https://www.%1%{REQUEST_URI} [L,NE,R=301]


RewriteRule ^Login /SalioViaje/Form/Login.php
RewriteRule ^Admin /SalioViaje/Form/Login_A.html
RewriteRule ^Register /SalioViaje/Form/Register.html
RewriteRule ^Forgot_Password /SalioViaje/Form/CambiarContrase√±a.html

RewriteRule ^Dashboard /SalioViaje/Panel/Dashboard.php
RewriteRule ^Usuarios /SalioViaje/Panel/Usuarios.php
RewriteRule ^Choferes /SalioViaje/Panel/Choferes.php
RewriteRule ^Empresas /SalioViaje/Panel/Empresas.php
RewriteRule ^Vehiculos /SalioViaje/Panel/Vehiculos.php
RewriteRule ^Settings /SalioViaje/Panel/Settings.php
RewriteRule ^Agendar /SalioViaje/Panel/Agendar.php
RewriteRule ^Viajes /SalioViaje/Panel/Agenda.php
RewriteRule ^FAQ_Edit /SalioViaje/Panel/FAQ.php

RewriteRule ^Success /Form/Success.html
RewriteRule ^Failed /Form/Failed.html

RewriteRule ^Viajar /SalioViaje/Agenda/Viajar.php
RewriteRule ^Oportunidad/([a-zA-Z0-9_\.]+)$ /SalioViaje/Agenda/Oportunidad.php?ID=$1
RewriteRule ^Solicitud/([a-zA-Z0-9_\.]+)$ /SalioViaje/Agenda/Oportunidad_Solicitud.php?ID=$1
RewriteRule ^Espera/([a-zA-Z0-9_\.]+)$ /SalioViaje/Agenda/Espera.php?ID=$1

RewriteRule ^Servicios /About/Servicios.html
RewriteRule ^Nosotros /About/Nosotros.html
RewriteRule ^FAQ /About/FAQ.html


ErrorDocument 404 /Error/404.html
ErrorDocument 403 /Error/403.html

Options All -Indexes