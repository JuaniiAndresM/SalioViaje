Options +FollowSymLinks
RewriteEngine on

DirectoryIndex index.html

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

#RewriteCond %{HTTPS} off [OR]
#RewriteCond %{HTTP_HOST} !^www\. [NC]
#RewriteCond %{HTTP_HOST} ^(?:www\.)?(.+)$ [NC]
#RewriteRule ^ https://www.%1%{REQUEST_URI} [L,NE,R=301]


RewriteRule ^Login /SalioViaje/Form/Login.php
RewriteRule ^Admin /SalioViaje/Form/Login_A.html
RewriteRule ^Register /SalioViaje/Form/Register.html

RewriteRule ^Dashboard /SalioViaje/Panel/Dashboard.php
RewriteRule ^Usuarios /SalioViaje/Panel/Usuarios.php
RewriteRule ^Empresas /SalioViaje/Panel/Empresas.php
RewriteRule ^Vehiculos /SalioViaje/Panel/Vehiculos.php
RewriteRule ^Settings /SalioViaje/Panel/Settings.php
RewriteRule ^Agendar /SalioViaje/Panel/AgendarViaje.php

RewriteRule ^Success /SalioViaje/Form/Success.html
RewriteRule ^Failed /SalioViaje/Form/Failed.html

RewriteRule ^Viajar /SalioViaje/Agenda/Viajar.html
RewriteRule ^Oportunidades /SalioViaje/Agenda/Oportunidades.html

RewriteRule ^Servicios /SalioViaje/About/Servicios.html
RewriteRule ^Nosotros /SalioViaje/About/Nosotros.html


ErrorDocument 404 /SalioViaje/Error/404.html
ErrorDocument 403 /SalioViaje/Error/403.html

Options All -Indexes
